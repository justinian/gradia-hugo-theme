{
  "name": "{{ .Site.Title }}",
  "_id": "__linktitlehash__({{ .LinkTitle }})",
  "pages": [
    {
      "sort": 100000,
      "name": "{{ .Title }}",
      "type": "text",
      "_id": "__linktitlehash__({{ .LinkTitle }}_1)",
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "{{ replaceRE `\n` `` .Content | safeHTML }}"
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "ownership": {
        "default": -1
      },
      "flags": {},
      "_stats": {
        "coreVersion": "11.315",
        "createdTime": {{ .PublishDate.Unix }},
        "modifiedTime": {{ .Lastmod.Unix }}
      },
      "_key": "!journal.pages!__linktitlehash__({{ .LinkTitle }}).__linktitlehash__({{ .LinkTitle }}_1)"
    }
    {{ $page := . }}
    {{- with resources.GetMatch "/maps/Overview.png" }}
    ,{
      "sort": 200000,
      "name": "Map",
      "type": "image",
      "_id": "__linktitlehash__({{ $page.LinkTitle }}_2)",
      "title": {
        "show": true,
        "level": 1
      },
      "image": {
        "caption": "Map of {{ $page.Site.Title }}"
      },
      "text": {
        "format": 1
      },
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": "modules/{{ $page.Site.Params.fvttName }}{{ $page.RelPermalink }}",
      "ownership": {
        "default": -1
      },
      "flags": {},
      "_stats": {
        "coreVersion": "11.315",
        "createdTime": {{ $page.PublishDate.Unix }},
        "modifiedTime": {{ $page.Lastmod.Unix }}
      },
      "_key": "!journal.pages!__linktitlehash__({{ $page.LinkTitle }}).__linktitlehash__({{ $page.LinkTitle }}_2)"
    }
    {{- end }}
  ],
  "folder": null,
  "sort": 0,
  "ownership": {
    "default": 0
  },
  "flags": {},
  "_stats": {
    "coreVersion": "11.315",
    "createdTime": {{ .PublishDate.Unix }},
    "modifiedTime": {{ .Lastmod.Unix }}
  },
  "_key": "!journal!__linktitlehash__({{ .LinkTitle }})"
}
